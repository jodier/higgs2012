double HiggsCrossSection::higgsprodxsecGGF(double mass, CrossSections::LHCEnergy energy)
{
  initializeH();
  if(energy != CrossSections::SevenTeV && energy != CrossSections::EightTeV) {
    std::cout << "Unknown LHC Energy" << std::endl;
    return 0.;
  }


  double xsec=0.;
  int i=0;
  if(energy == CrossSections::SevenTeV) {
    if(mass<90. || mass>1000.) return 0.;
    for(i=0;i<207;i++) if(m_signalmasses[i]>=mass) break;
    xsec = m_signalggxsec[i-1] + 
      (m_signalggxsec[i]-m_signalggxsec[i-1])/(m_signalmasses[i]-m_signalmasses[i-1])*
      (mass-m_signalmasses[i-1]);  
  }
  else { // EightTeV
    if(mass<80. || mass>1000.) return 0.;
    for(i=0;i<233;i++) if(m_signalmasses_8TeV[i]>=mass) break;
    xsec = m_signalggxsec_8TeV[i-1] + 
      (m_signalggxsec_8TeV[i]-m_signalggxsec_8TeV[i-1])/(m_signalmasses_8TeV[i]-m_signalmasses_8TeV[i-1])*
      (mass-m_signalmasses_8TeV[i-1]);  
  }
  
  return xsec*1.e3; // in fb

}

