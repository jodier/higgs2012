double HiggsCrossSection::interpolate(unsigned int n, 
                                      const std::vector<double>& x, 
                                      const std::vector<double>& y, 
                                      double mass)
{
  double  result = 0.;
  unsigned int i = 0;
  for(i = 0; i < n; i++) {
    if(x[i] >= mass)
      break;
  }
  result = y[i-1] + (y[i] - y[i-1])/(x[i] - x[i-1])*(mass - x[i-1]);
  return result;
}
